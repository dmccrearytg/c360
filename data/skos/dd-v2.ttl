@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://example.org/ns#000> skos:altLabel "Books",
        "Generalities",
        "Internet",
        "Libraries" ;
    skos:definition "Books and other compilations that do not fit other categories, including computer science and information." ;
    skos:inScheme <http://example.org/ns#DeweyDecimalClassification> ;
    skos:prefLabel "General works, Computer science and Information" ;
    skos:topConceptOf <http://example.org/ns#DeweyDecimalClassification> .

